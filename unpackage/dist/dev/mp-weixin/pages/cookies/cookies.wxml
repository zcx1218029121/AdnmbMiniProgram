<view><scroll-view class="{{['page',modalName!=null?'show':'']}}" scroll-y="{{modalName==null}}"><radio-group data-event-opts="{{[['change',[['RadioChange',['$event']]]]]}}" class="block" bindchange="__e"><view class="{{['cu-list menu',menuBorder?'sm-border':'',menuCard?'card-menu margin-top':'']}}"><block wx:for="{{cookies}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',menuArrow?'arrow':'']}}"><view class="content"><text class="text-grey">{{item.name}}</text></view><view class="action"><radio class="round bg-green shadow" value="{{item.userhash}}"></radio></view></view></block></view></radio-group><button class="cu-btn block bg-green margin-tb-sm lg" data-target="Modal" data-event-opts="{{[['tap',[['showModal',['$event']]]]]}}" bindtap="__e"><text class="cuIcon-add"></text>添加饼干</button><button class="cu-btn block bg-blue margin-tb-sm lg" data-target="Modal" data-event-opts="{{[['tap',[['addUserHash',['$event']]]]]}}" bindtap="__e">设为默认饼干</button><view class="{{['cu-modal',modalName=='Modal'?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content">添加饼干</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl"><view class="cu-form-group"><input placeholder="饼干名" name="input" data-event-opts="{{[['input',[['inputCookiesName',['$event']]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="cu-form-group"><input placeholder="userhash" name="input" data-event-opts="{{[['input',[['inputUserHash',['$event']]]]]}}" value="{{userhash}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['addCookies',['$event']]]]]}}" class="cu-btn block bg-green margin-tb-sm lg" bindtap="__e"><text class="cuIcon-add"></text>确定</button></view></view></view></scroll-view></view>