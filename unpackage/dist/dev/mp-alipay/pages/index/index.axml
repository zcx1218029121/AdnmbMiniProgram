<view class="pager-content"><scroll-view class="nav-bar" id="nav-bar" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block a:for="{{tabBars}}" a:for-item="item" a:for-index="index" a:key="id"><view class="{{(('nav-item')+' '+((index===tabCurrentIndex)?'current':''))}}" id="{{'tab'+index}}" data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" onTap="__e">{{item.name}}</view></block></scroll-view><mix-pulldown-refresh class="panel-content" vue-id="1" top="{{200}}" ref="__r" data-ref="mixPulldownRefresh" data-event-opts="{{[['^refresh',[['onPulldownReresh']]],['^setEnableScroll',[['setEnableScroll']]]]}}" onRefresh="__e" onSetEnableScroll="__e" onVueInit="__l"><swiper class="swiper-box" id="swiper" duration="{{300}}" current="{{tabCurrentIndex}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" onChange="__e"><block a:for="{{tabBars}}" a:for-item="tabItem" a:for-index="__i0__" a:key="id"><swiper-item><scroll-view class="panel-scroll-box" scroll-y="{{enableScroll}}" data-event-opts="{{[['scrollToLower',[['loadMore',['$event']]]]]}}" onScrollToLower="__e"><block a:for="{{tabItem.newsList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['navToDetails',['$0'],[[['tabBars','id',tabItem.id],['newsList','',index]]]]]]]}}" class="cu-card dynamic" onTap="__e"><view class="cu-item shadow"><view class="cu-list menu-avatar"><view class="cu-item"><view class="cu-avatar lg round bg-red">{{''+item.icon}}</view><view class="content flex-sub"><view>{{item.userid}}</view><view class="text-gray text-sm flex justify-between"><rich-text nodes="{{item.now}}"></rich-text></view></view></view></view><view class="text-content" style="white-space:pre-wrap;">{{''+item.content+''}}</view><view class="{{(('grid flex-sub padding-lr')+' '+(isCard?'col-3 grid-square':'col-1'))}}"><block a:for="{{isCard?9:1}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('bg-img')+' '+(isCard?'':'only-img'))}}" style="background-image:url(https://ossweb-img.qq.com/images/lol/web201310/skin/big10006.jpg);"></view></block></view><view class="text-gray text-sm text-right padding"><text class="cuIcon-messagefill margin-lr-xs"></text>{{''+item.replyCount+''}}</view></view></view></block><mix-load-more vue-id="{{('2-'+__i0__)+','+('1')}}" status="{{tabItem.loadMoreStatus}}" onVueInit="__l"></mix-load-more></scroll-view></swiper-item></block></swiper></mix-pulldown-refresh><view class="cu-bar tabbar margin-bottom-xl bg-white"><view class="action text-green"><view class="cuIcon-homefill"></view>首页</view><view class="action text-gray add-action"><button class="cu-btn cuIcon-add bg-green shadow"></button>发布</view><view class="action text-gray"><view class="cuIcon-my"></view>我的</view></view></view>